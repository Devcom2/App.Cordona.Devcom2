<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" xmlns:gap="http://phonegap.com/ns/1.0" id="io.cordova.Devcom2" version="0.0.1.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Devcom2</name>
  <description>
    A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.
  </description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">
    Apache Cordova Team
  </author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="Fullscreen" value="true" />
  <icon src="images/Logo.png" gap:platform="android" gap:density="ldpi" />
	<icon src="images/Logo.png" gap:platform="android" gap:density="mdpi" />
	<icon src="images/Logo.png" gap:platform="android" gap:density="hdpi" />
	<icon src="images/Logo.png" gap:platform="android" gap:density="xhdpi" />
	<icon src="images/Logo.png" gap:platform="android" gap:density="xxhdpi" />

  <vs:features>
    <vs:feature>org.apache.cordova.splashscreen@0.3.1</vs:feature>
    <vs:feature>org.apache.cordova.vibration@0.3.9</vs:feature>
    <vs:feature>org.apache.cordova.globalization@0.2.8</vs:feature>
    <vs:feature>org.apache.cordova.camera@0.3.0</vs:feature>
    <vs:feature>org.apache.cordova.battery-status@0.2.8</vs:feature>
  </vs:features>
  <vs:platformSpecificValues />
<cordova>
  <!--
    access elements control the Android whitelist.
    Domains are assumed blocked unless set otherwise
     -->

  <access uri="devcom2.api.mymobileapp.be" subdomains="true" />
  
</cordova>
  <access uri="devcom2.api.mymobileapp.be" subdomains="true" />
</widget>